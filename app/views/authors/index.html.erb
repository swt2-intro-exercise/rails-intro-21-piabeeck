<h1>All Authors bla List bla</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Homepage</th>
  </tr>
 
  <% @authors.each do |author| %>
    <tr>
      <td><%= author.name %></td>
      <td><%= link_to author.homepage, author.homepage %></td>
      <td><%= link_to 'Show', author_path(author) %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to 'New', new_author_path%></p>